<!DOCTYPE html>
<html>
<head>
    <title>Translating Plugins: Your repositories on Github</title>
    %= include 'app/part_user_head'
</head>
<body>

    %= include 'app/part_user_navbar'

<div class="container">

    %= include 'app/part_user_alert'

    <p><strong><%= $user_data->{name} %> - Resps</strong></p>
    <ul>
        % foreach my $rec (@$resp_data) {
            <li>
                <%= $rec->{fullname} %> <a href="<%= url_for('/app/home/do_register_resp')->query(name => $rec->{fullname} ) %>">Register</a><br/>
                <%= $rec->{description} %>
            </li>
        % }
    </ul>

</div> <!-- /container -->

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
<script src="<%= url_for('/bootstrap/js/bootstrap.min.js') %>"></script>

</body>
</html>
